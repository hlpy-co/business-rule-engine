rule "is_auction_arval_priority"
priority 1
when
    business_partner == "arval"
    event == "is_auction"
then
    print("is_auction with priority")
    if 1!=1:
        print("non dovrebbe essere stampato")
        set_variable("hello", "never")
    exclude("event", "is_auction")
    set_variable("priority", "True")
end